/**app.wxss**/
/**
 *  1rem==38rpx==19px
 */
page{
  background-color: #eceff6;
  font-family: arial;
}
.container {
  box-sizing: border-box;
  line-height: 1.5;
  font-size: 32rpx;
  padding-bottom: 20rpx;
  font-size: .8rem;
} 
.main{  
  background-color: #fff;
  padding: 20rpx;
}
.container .h1{
  font-size: 1rem;
  line-height: 2rem;
}
input{
  padding: 10rpx;
  border: 1px solid #ddd;
}
.container .tip{
  font-size: 0.8rem;
  line-height: 1.6rem;
  color: #999;
}
.container .tips{
  padding: 20rpx;
  font-size: .8rem;
  border-bottom: 1rpx solid #ddd;
}
.hr{
  height: 0;
  border-bottom: 1rpx solid #ddd;
  margin: 30rpx 0;
}
.placeholder{
  color: #bbb;
}
.fr{
  float: right;
}

.input-el{
  display: flex;
  border: 1px solid #ddd;
  padding: 10rpx;
  flex-direction: row;
  color: #666;
  line-height: 1.5rem;
  align-content: center;
}
.input-el label{
  /*background-color: #fe0;*/
}
.input-el input{
  flex:1;
  border: none;
  padding: 0;
  height: 1.5rem;
  /*background-color: #f00;*/
  padding-left: .5rem;
}
.input-el text{
  line-height: 1.4rem;
}

.button-v{
  margin-top: 1rem;
  padding: 20rpx;
}
.button-v button{
  background-color: #007aff;
  color: #fff;
}

.rem{
  display: inline-block;
  width: .8rem;
}


/**
 * step style
 */
.step-block{
  background-color: #fff;
}
.step-block .step{
  padding: 1rem 7.81% 0;  
}
.step-block .inner{
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  padding-top: 9.26%;
  position: relative;
  width: 100%;
}
.step-block .inner .progress{
  position: absolute;
  top:40%;
  left: 5%;
  height: 25%;
  background-color: #007aff;
}
.step-block .inner .list{
  display: flex;
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  justify-content: space-between;
}
.step-block .inner .list view{
  width: 9.26%;
  text-align: center;
  height: 100%;
  line-height: 1.5rem;
}
.step-block .titles{
  display: flex;
  justify-content: space-between;
  line-height: 2rem;
}
.step-block .titles view{
  flex:1;
  text-align: center;
}
.step1 .list .num_1,
.step2 .list .num_2,
.step3 .list .num_3,
.step4 .list .num_4{
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 76%;
}
.step2 .list .num_1,
.step3 .list .num_1,
.step3 .list .num_2,
.step4 .list .num_1,
.step4 .list .num_2,
.step4 .list .num_3{
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 76%;
}
.step1 .list .num_1 text,
.step2 .list .num_1 text,
.step2 .list .num_2 text,
.step3 .list .num_1 text,
.step3 .list .num_2 text,
.step3 .list .num_3 text,
.step4 .list .num_1 text,
.step4 .list .num_2 text,
.step4 .list .num_3 text,
.step4 .list .num_4 text{
  display: none;
}
.step1 .inner .progress{
  width: 0;
}
.step2 .inner .progress{
  width: 27%;
}
.step3 .inner .progress{
  width: 58%;
}
.step4 .inner .progress{
  width: 88%;
}
.step1 .title_1,
.step2 .title_2,
.step3 .title_3,
.step4 .title_4{
  color:#007aff;
}
/**
 * step style end
 */