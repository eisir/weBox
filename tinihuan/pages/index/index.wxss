.times{
  display: flex;
  justify-content: flex-start;
}
.times view{
  padding: 0.5rem;
  border: 1px solid #007aff;
  background-color: #fff;
  margin-right: 0.4rem;
  /*border-radius: 6rpx;*/
}
.times view.selected{
  background-color: #007aff;
  color: #fff;
}

