/* pages/applyLoan/applyLoan.wxss */
.input-el{
  border: none;
  border-bottom: 1px solid #ddd;
}

.main .input-el:last-child{
  border-bottom: none;
}
.main .input-el strong{
  text-align: right;
  flex:1;
}
.send-btn{
  background-color: #007aff;
  color: #fff;
  border-radius: 6rpx;
  padding: 0 1rem;
  line-height: 1.5rem;
}
.h1{
  padding: 0 1rem;
}
.agreement-list{
  width: 90.63%;
  margin: 1rem auto 0;
}
.agreement .icon{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
  vertical-align: middle;
  position: relative;
  top: -4rpx;
  margin-right: 10rpx;

}
.disagree .icon{
  background-image: url(data:image/png;base64,);
}
navigator {
  display: inline-block;
}
.container .tips{
  border: none;
}
.button-v{
  margin-top: 0; 
}